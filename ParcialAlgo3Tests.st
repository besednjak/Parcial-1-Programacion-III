TestCase subclass: #TestVehiculoTerrestre	instanceVariableNames: ''	classVariableNames: ''	package: 'ParcialAlgo3Tests'!!TestVehiculoTerrestre methodsFor: 'tests' stamp: 'MarioBesednjak 6/30/2020 20:20'!test01MotonetaAvanza80kmEnDosHs	"Una Motoneta deberia recorrer 80 km en 2 horas"	| unaMotoneta |	unaMotoneta := VehiculoTerrestre crearMotoneta .		self assert: (unaMotoneta cantidadKmAlAvanzarHoras: 2 "horas") equals: 80 "Km".! !!TestVehiculoTerrestre methodsFor: 'tests' stamp: 'MarioBesednjak 6/30/2020 20:25'!test03VehiculoAveriadoNoPuedeAvanzar	| unaMotoneta |	unaMotoneta := VehiculoTerrestre crearMotoneta .	unaMotoneta romperVehiculo.		self  should: [unaMotoneta cantidadKmAlAvanzarHoras: 2 "horas"] raise: UnVehiculoAveriadoNoPuedeAvanzarError.! !!TestVehiculoTerrestre methodsFor: 'tests' stamp: 'MarioBesednjak 6/30/2020 20:21'!test02MotoDeCarrerasAvanza480kmEnDosHs	"Una Moto de carreras deberia recorrer 480 km en 2 horas"	| unaMotoDeCarreras |	unaMotoDeCarreras := VehiculoTerrestre crearMotoDeCarreras .		self assert: (unaMotoDeCarreras cantidadKmAlAvanzarHoras: 2 "horas") equals: 480 "Km".! !